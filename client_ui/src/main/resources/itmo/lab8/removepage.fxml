<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox styleClass="main-class" alignment="TOP_CENTER" fillWidth="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="151.0" prefWidth="228.0" spacing="20.0"
      xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="remove_label" text="Введите id" textFill="#dadada">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <TextField fx:id="movie_id" style="-fx-background-color: #474747;"/>
        <Button fx:id="remove_button" onAction="#onRemoveButtonClick" mnemonicParsing="false"
                style="-fx-background-color: #474747;" text="Удалить" textFill="#dadada"/>
    </children>
    <padding>
        <Insets top="20.0"/>
    </padding>
</VBox>
