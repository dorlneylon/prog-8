<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="136.0" prefWidth="255.0" spacing="10.0" style="-fx-background-color: #363636;"
      xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="choose_mpaa_label" alignment="TOP_CENTER" prefHeight="25.0" prefWidth="293.0" text="Выберите MPAA"
               textFill="#dadada">
            <font>
                <Font size="15.0"/>
            </font>
            <VBox.margin>
                <Insets top="20.0"/>
            </VBox.margin>
        </Label>
        <ComboBox fx:id="combobox_mpaa" prefWidth="150.0" style="-fx-background-color: #474747;">
            <VBox.margin>
                <Insets top="-5.0"/>
            </VBox.margin>
        </ComboBox>
        <Button fx:id="remove_mpaa_button" onAction="#onRemoveButtonClick" alignment="CENTER" contentDisplay="CENTER"
                mnemonicParsing="false" style="-fx-background-color: #474747;" text="Удалить" textFill="#dadada"/>
    </children>
</VBox>
